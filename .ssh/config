Host *.cloud.internal.lecat.pro !adm.cloud.internal.lecat.pro
        ProxyJump adm.cloud.internal.lecat.pro

Host *.dmz.internal.lecat.pro !adm.dmz.internal.lecat.pro
        ProxyJump adm.dmz.internal.lecat.pro

Host *.dce.internal.lecat.pro !adm.dce.internal.lecat.pro
        ProxyJump adm.dce.internal.lecat.pro

Host storage
	HostName storage-sec
	User root

Host storage-ltsp
	HostName storage-ltsp-sec
	User root

Host gw-* gw.*
	User vyos

Host core-* 10.10.40.100 10.10.40.101
	User root
	Ciphers aes256-cbc
        KexAlgorithms diffie-hellman-group1-sha1
        HostKeyAlgorithms +ssh-dss

Host * !adm* !10.10.*
	ProxyJump adm-internal.internal.lecat.pro

Host *
	ForwardAgent yes
        AddKeysToAgent yes
        IdentityFile ~/.ssh/id_rsa_torque
	UseKeychain yes
	StrictHostKeyChecking no
	HashKnownHosts no
