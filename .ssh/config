# -- TORQUE
Host *.dmz.internal.lecat.pro !adm.dmz.internal.lecat.pro
        ProxyJump adm.dmz.internal.lecat.pro

Host gitlab.dmz.internal.lecat.pro
	Port 222

Host *.dce.internal.lecat.pro !adm.dce.internal.lecat.pro
        ProxyJump adm.dce.internal.lecat.pro

Host *.internal.lecat.pro *.home.lecat.network !*.*.internal.lecat.pro !adm.secure.home.lecat.network
	ProxyJump adm.secure.home.lecat.network

Host *.osc.lecat.network !adm.osc.lecat.network
	ProxyJump adm.secure.osc.lecat.network

Host *.osc-us.lecat.network !adm.osc-us.lecat.network
	ProxyJump adm.osc-us.lecat.network

Host gw.internal.lecat.pro gw-*.internal.lecat.pro ap-*.internal.lecat.pro fw-*.internal.lecat.pro sw-*.internal.lecat.pro
	IdentityFile ~/.ssh/id_rsa_fw
        IdentitiesOnly yes

Host gw.internal.lecat.pro gw-*.internal.lecat.pro
	User vyos

Host ap-*.internal.lecat.pro
	User prdadm

Host fw-*.internal.lecat.pro
	User root

Host sw-*.internal.lecat.pro ap-*.internal.lecat.pro
	User root
	IdentityFile ~/.ssh/id_rsa_fw
	IdentitiesOnly yes
	Ciphers aes256-ctr,aes256-cbc
        KexAlgorithms +diffie-hellman-group1-sha1
        HostKeyAlgorithms +ssh-dss

Host *.internal.lecat.pro *.lecat.network !sw-*.internal.lecat.pro
	User romain
	IdentityFile ~/.ssh/id_rsa

Host esxi*.internal.lecat.pro vcenter.internal.lecat.pro
	User root
	IdentityFile ~/.ssh/id_rsa

# -- WORK
Host par1-* nj1-* hk1-* sv1-* par4-* nj2-* jpn1-* top1-* dshq1-* dv1-* dv2-* in1-* in2-*
        User romain.lecat
        IdentityFile ~/.ssh/id_rsa_outscale

Host par1-* !par1-adm.admin !par1-ops-* !par1-balabit-*
        ProxyJump par1-adm.admin

Host par4-* !par4-adm.admin
        ProxyJump par4-adm.admin

Host sv1-* !sv1-adm.admin
        ProxyJump sv1-adm.admin

Host hk1-* !hk1-adm.admin
        ProxyJump hk1-adm.admin

Host hk2-* !hk2-adm.admin
        ProxyJump hk2-adm.admin

Host nj1-* !nj1-adm.admin
        ProxyJump nj1-adm.admin

Host nj2-* !nj2-adm.admin
        ProxyJump nj2-adm.admin

Host jpn1-* !jpn1-adm.admin
	ProxyJump jpn1-adm.admin

Host top1-* !top1-adm.admin
        ProxyJump top1-adm.admin

Host dshq1-* !dshq1-adm-2.admin
        ProxyJump dshq1-adm-2.admin

Host dv1-* dv2-* !dv1-adm.admin
        ProxyJump dv1-adm.admin

Host in1-* !in1-adm.admin
        ProxyJump in1-adm.admin

Host in2-* !in2-adm.admin
        ProxyJump in2-adm.admin

# -- GENERIC
Host *
	ForwardAgent yes
        AddKeysToAgent yes
	UseKeychain yes
	StrictHostKeyChecking no
	HashKnownHosts no
